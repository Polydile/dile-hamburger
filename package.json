{
  "name": "dile-hamburger",
  "version": "2.0.1",
  "description": "Webcomponent dile-hamburger following open-wc recommendations",
  "author": "@midesweb",
  "homepage": "https://github.com/Polydile/dile-hamburger",
  "license": "MIT",
  "repository": "https://github.com/Polydile/dile-hamburger.git",
  "scripts": {
    "start": "es-dev-server --app-index demo/index.html --node-resolve --open --watch",
    "start:compatibility": "es-dev-server --app-index demo/index.html --compatibility all --node-resolve --open --watch",
    "site:build": "npm run storybook:build",
    "storybook": "start-storybook -p 9001",
    "storybook:build": "build-storybook -o _site -s storybook-static"
  },
  "dependencies": {
    "lit-html": "^1.0.0",
    "lit-element": "^2.0.1"
  },
  "devDependencies": {
    "es-dev-server": "^1.5.0",
    "@open-wc/demoing-storybook": "^2.4.7"
  },
  "keywords": [
    "icon",
    "hamburguer",
    "LitElement",
    "menu",
    "Web Components",
    "animation"
  ]
}
